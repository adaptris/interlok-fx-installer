<project>
  <version>3.8.0-RELEASE</version>
  <name>Interlok/CSV</name>
  <url>http://interlok.adaptris.net/interlok-docs/cookbook-csv-transform.html</url>
  <description>Handling CSV files using Interlok; transform to XML, splitting, JDBC utilities</description>
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.adaptris</groupId>
  <artifactId>interlok-csv</artifactId>
  <packaging>jar</packaging>
  <properties>
    <externalUrl>https://en.wikipedia.org/wiki/Comma-separated_values</externalUrl>
    <notes>Requires additional jars not automatically delivered</notes>
    <deprecated>Some message about this component being deprecated</deprecated>
    <license>true</license>
    <target>3.8.0+</target>
    <tags>csv,transform,jdbc</tags>
    <logo>/interlok/img/optional/csv.png</logo>
    <adapters>com.adaptris.core.transform.csv.SimpleCsvToXmlAdapter</adapters>
    <channels>com.adaptris.core.transform.csv.SimpleCsvToXmlChannel</channels>
    <workflows>com.adaptris.core.transform.csv.SimpleCsvToXmlWorkflow</workflows>
    <services>com.adaptris.core.transform.csv.SimpleCsvToXmlTransformService,com.adaptris.core.transform.csv.SimpleXmlToCsvTransformService</services>
    <consumers>com.adaptris.core.transform.csv.SimpleCsvConsumer</consumers>
    <producers>com.adaptris.core.transform.csv.SimpleCsvProducer</producers>
    <interceptors>com.adaptris.core.transform.csv.SimpleCsvInterceptor</interceptors>
    <serviceCollections>com.adaptris.core.transform.csv.SimpleCsvCollection</serviceCollections>
    <connections>com.adaptris.core.transform.csv.SimpleCsvConnection</connections>
    <advanced>com.adaptris.core.transform.csv.BasicFormatBuilder,com.adaptris.core.transform.csv.CustomFormatBuilder,com.adaptris.csv.jdbc.BasicPreferenceBuilder,com.adaptris.csv.jdbc.ConfiguredColumnFilter,com.adaptris.csv.jdbc.CsvResultSetTranslator,com.adaptris.csv.jdbc.MetadataColumnFilter</advanced>
  </properties>
  <dependencies>
    <dependency>
      <groupId>com.adaptris</groupId>
      <artifactId>interlok-core</artifactId>
      <version>3.9-SNAPSHOT</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.adaptris</groupId>
      <artifactId>interlok-common</artifactId>
      <version>3.9-SNAPSHOT</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.adaptris</groupId>
      <artifactId>interlok-stax</artifactId>
      <version>3.9-SNAPSHOT</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.adaptris</groupId>
      <artifactId>interlok-stubs</artifactId>
      <version>3.9-SNAPSHOT</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.apache.commons</groupId>
      <artifactId>commons-csv</artifactId>
      <version>1.5</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>net.sf.supercsv</groupId>
      <artifactId>super-csv</artifactId>
      <version>2.4.0</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.adaptris</groupId>
      <artifactId>interlok-core-apt</artifactId>
      <version>3.9-SNAPSHOT</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.apache.velocity</groupId>
      <artifactId>velocity</artifactId>
      <version>1.7</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.slf4j</groupId>
      <artifactId>slf4j-api</artifactId>
      <version>1.7.29</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.slf4j</groupId>
      <artifactId>jcl-over-slf4j</artifactId>
      <version>1.7.29</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <groupId>org.slf4j</groupId>
      <artifactId>slf4j-log4j12</artifactId>
      <version>1.7.29</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>1.2.17</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.11</version>
      <scope>test</scope>
    </dependency>
    <org.apache.maven.model.Dependency>
      <groupId>org.apache.derby</groupId>
      <artifactId>derby</artifactId>
      <version>10.14.2.0</version>
      <type>jar</type>
      <scope>test</scope>
    </org.apache.maven.model.Dependency>
  </dependencies>
</project>